pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 5
            }
        }
      stage('Build') {
            steps {
                echo 'Build'
                sleep 5
            }
        }
            stage('Test') {
            steps {
                echo 'test'
                sleep 5
            }
        }
   }
}

